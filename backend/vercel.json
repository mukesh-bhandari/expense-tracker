{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "dist/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(login|expenses.*)",
      "dest": "server.js"
    },
    {
      "src": "/(.+)",
      "dest": "dist/index.html"
    }
  ]
}